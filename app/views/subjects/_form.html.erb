<%= error_messages_for(f.object) %>

<table summary="Subjects form fields">

<tr>
<th>Name</th>
<td><%= f.text_field(:name) %></td>
</tr>

<tr>
<th>Position</th>
<td><%= f.select(:position, 1..@subject_count) %></td>
</tr>

<tr>
<th>Visible</th>
<td><%= f.check_box(:visible) %></td>
</tr>

tr>
<th>created_at</th>
<td><%= f.datetime_select(:created_at) %></td>
</tr>


</table>